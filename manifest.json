{
  "name": "Guestdrop",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Browse Massdrop without an account.",
  "homepage_url": "http://zackboehm.com/projects/guestdrop",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "webRequest",
    "*://*.massdrop.com/*"
  ],
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  }
}