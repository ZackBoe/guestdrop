{
  "name": "Guestdrop",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Browse Massdrop without an account",
  "homepage_url": "http://zackboehm.com/projects/guestdrop",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": ["https://*.massdrop.com/*"],
  "content_scripts": [
    {
      "matches": ["https://*.massdrop.com/*"],
      "js": ["src/inject/inject.js"]
    }
  ]
}